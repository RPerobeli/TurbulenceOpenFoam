FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10;//1.994e-5;//7.15542e-7;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    airfoil
    {
        type            omegaWallFunction;
        value           uniform 0;
    }
    top
    {
        type            fixedValue;
        value           $internalField;//7.15542e-7;
    }
    bottom
    {
        type           fixedValue;
        value           $internalField;//7.15542e-7;
    }
    front
    {
        type            empty;
    }
   back
   {
        type            empty;
    }
}


// ************************************************************************* //
