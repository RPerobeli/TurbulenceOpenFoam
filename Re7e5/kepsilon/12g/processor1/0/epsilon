FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.3018;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    airfoil
    {
        type            epsilonWallFunction;
        value           uniform 0.3018;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.3018;
    }
}


