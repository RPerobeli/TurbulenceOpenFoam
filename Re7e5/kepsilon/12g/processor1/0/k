FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    airfoil
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1.5;
    }
}


