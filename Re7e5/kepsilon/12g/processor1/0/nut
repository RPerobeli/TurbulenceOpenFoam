FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.6708;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    airfoil
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.6708;
    }
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.6708;
    }
}


